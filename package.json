    "lint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "@antfu/eslint-config": "latest",
    "@types/bun": "latest",
    "eslint": "latest",
    "typescript": "beta"
  },
  "packageManager": "bun@1.3.9"
}
