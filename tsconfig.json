{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022"],
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "noUncheckedSideEffectImports": true
  },
  "exclude": ["node_modules", "**/node_modules", "**/dist", "**/.output", "**/.vinxi"]
}
